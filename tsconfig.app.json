{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/types/**/*.d.ts"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"verbatimModuleSyntax": false,
		"composite": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"types": ["./src/types/wind"],
		"allowJs": true
	}
}
